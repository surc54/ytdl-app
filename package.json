{
    "name": "ytdl",
    "description": "YouTube downloader",
    "version": "0.1.0",
    "author": {
        "name": "Adithya Haridas"
    },
    "private": true,
    "build": {
        "appId": "com.hadithya.ytdl",
        "productName": "YTDL",
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "win": {
            "icon": "build/logo.ico"
        }
    },
    "main": "public/electron.js",
    "homepage": ".",
    "dependencies": {
        "@material-ui/core": "^4.1.3",
        "@material-ui/styles": "^4.1.2",
        "axios": "^0.19.0",
        "filenamify": "^4.1.0",
        "lodash": "^4.17.11",
        "material-design-icons": "^3.0.1",
        "node-sass": "^4.12.0",
        "notistack": "^0.8.9",
        "react": "^16.8.6",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "react-virtualized": "^9.21.1",
        "react-virtualized-auto-sizer": "^1.0.2",
        "react-virtuoso": "^0.9.0",
        "recompose": "^0.30.0",
        "redux": "^4.0.1",
        "redux-form": "^8.2.4",
        "redux-thunk": "^2.3.0",
        "typeface-roboto": "^0.0.54",
        "typescript": "3.5.2",
        "youtube-playlist": "^1.0.2",
        "ytdl-core": "^0.29.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "electron": "electron ./public/electron.js",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "pack": "electron-builder --dir",
        "dist1": "electron-builder --em.main=build/electron.js",
        "dist": "electron-builder"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "electron": "^13.6.6",
        "electron-builder": "^21.1.1",
        "prettier": "^1.18.2"
    }
}
